{
  "name": "@libit/josa",
  "version": "0.3.7",
  "description": "JSON Object Signing and Authentication JS library",
  "author": "Mindary",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean libit-josa*.tgz dist *.tsbuildinfo package",
