    "tily": "^2.7.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.1",
    "@loopback/eslint-config": "^10.2.1",
    "@loopback/testlab": "^3.4.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.14.0"
  },
  "keywords": [],
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
