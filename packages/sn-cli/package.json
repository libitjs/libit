{
  "name": "@libit/sn-cli",
  "description": "The sn command line tool",
  "version": "0.3.1",
  "keywords": [
    "serial",
    "number",
    "serial-number",
    "sn",
    "hardware",
    "cpu",
    "uuid",
    "machine",
    "cli",
    "command",
    "line",
    "interface",
    "tool",
    "tools"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "Mindary",
  "license": "MIT",
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean libit-sn-cli*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs  \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha --allow-console-logs  \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf libit-sn-cli*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@caporal/core": "^2.0.2",
    "@libit/sn": "^0.3.1",
    "strong-globalize": "^6.0.5"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.0",
    "@loopback/eslint-config": "^11.0.0",
    "@loopback/testlab": "^3.4.2",
    "@types/node": "^16.4.10",
    "strong-globalize-cli": "^7.1.0",
    "ts-node": "^10.1.0"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "intl",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  }
}
